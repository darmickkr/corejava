package s4;
//Write a Java program to create a class called BankAccount with private instance variables accountNumber and balance.
//Provide public getter and setter methods to access and modify theseÂ variables.
public class BankAccount {
	private int accountNumber;
	private int balance;
	
	public int getaccountNumber()
	{
		return accountNumber;
	}
	public int getbalance()
	{
		return balance;
	}
	public void setaccountNumber(int num)
	{
		this.accountNumber = num;
	}
	public void setbalance(int bal)
	{
		this.balance = bal;
	}
	public static void main(String args[])
	{
		BankAccount b1 = new BankAccount();
//		System.out.println(b1.getaccountNumber()+","+b1.getbalance());
		b1.setaccountNumber(105);
		b1.setbalance(5000);
		//System.out.println(b1.getaccountNumber()+","+b1.getbalance());

	}

}
