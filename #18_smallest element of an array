//Largest element of an array
public class c3_largest_element_of_an_array {
    public static void main(String args[])
    {
        int[] num ={5,6,9,10,1,100};
        int max =num[0];
        for(int i =1;i<num.length;i++)
        {
            if(max<num[i])
            {
                max = num[i];
            }
        }
        System.out.println(max);

    }
}
